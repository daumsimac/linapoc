{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "koreacentral"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "SSH",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "22"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "Dev-DB-Subnet"
        },
        "publicIpAddressName": {
            "value": "DEV-WAS-Backend-VM01-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "virtualMachineName": {
            "value": "DEV-WAS-Backend-VM01"
        },
        "virtualMachineComputerName": {
            "value": "DEV-WAS-Backend-VM01"
        },
        "virtualMachineRG": {
            "value": "DEV-RG"
        },
        "osDiskType": {
            "value": "StandardSSD_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_B2s"
        },
        "adminUsername": {
            "value": "rootadmin"
        },
        "adminPassword": {
            "value": null   
        }
    }
}
